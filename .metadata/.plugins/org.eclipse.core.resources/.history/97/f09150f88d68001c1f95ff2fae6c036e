import java.awt.*;
import java.awt.event.*;
import javax.swing.*;
public class MouseEvents extends JFrame implements MouseListener
{
 JLabel txt;
 public MouseEvents()
 {
	 super("Window with MouseEvents");
	 setLayout(new FlowLayout());
	 txt=new JLabel();
	 add(txt);
	 addMouseListener(this);
	 setSize(500,500);
 }
}
